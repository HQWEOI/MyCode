package practice_11_21;

public class Practice04 {

	public static void main(String[] args) {
		int[] score={0,0,1,2,3,5,4,5,2,8,7,6,9,5,4,8,3,1,0,2,4,8,7,9,5,2,1,2,3,9};
		int[] count;
		for(int i=0;i<score.length;i++) {
			switch(score[i]) {
			case 0:
				score[0] += 1;
				break;
			case 1:
				score[1] += 1;
				break;
			case 2:
				score[2] += 1;
				break;
			case 3:
				score[3] += 1;
				break;
			case 4:
				score[4] += 1;
				break;
			case 5:
				score[5] += 1;
				break;
			case 6:
				score[6] += 1;
				break;
			case 7:
				score[7] += 1;
				break;
			case 8:
				score[8] += 1;
				break;
			case 9:
				score[9] += 1;
				break;
			}
		}
		for(int j=0;j<10;j++) {
			System.out.println(score[j]);
		}
	}
}
